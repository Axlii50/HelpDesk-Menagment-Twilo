@model MenagmentViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HelpDesk_Menagment_Twilo</title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HelpDesk_Menagment_Twilo.styles.css" asp-append-version="true" />
</head>
<body>
    <section class="Page">
    <header class="header">
        <div class="container-fluid container__header">
            <nav class="navigation">
                <ul class="navigation__items">
                    <li class="navigation__item">
                        <!--Tutaj zrob przejscia-->
                            <a class="navigation__link" asp-action="Index" asp-controller="HelpDesk" asp-route-AccountID="@Model.AccountID">HelpDesk</a>
                        <a class="navigation__link" href="#">Zarządzanie</a>
                    </li>
                </ul>
            </nav>
        </div>
    </header>

    @RenderBody()

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    </section>
</body>
</html>
